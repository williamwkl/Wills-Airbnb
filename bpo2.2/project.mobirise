              "position": "relative",
              "margin": "0 20px",
              "@media (max-width: 992px)": {
                "margin": "0 10px"
              },
              ".card": {
                "position": "relative",
                "padding": "0",
                ".decor-wrapper": {
                  "position": "absolute",
                  "top": "0",
                  "left": "-11rem",
                  "width": "3000px",
                  "height": "2000px",
                  "border": "40px solid @decor",
                  "border-radius": "15rem",
                  "pointer-events": "none",
                  "@media (max-width: 1300px)": {
                    "left": "-8rem"
                  },
                  "& when not (@reverseContent)": {
                    "left": "auto",
                    "right": "-11rem",
                    "@media (max-width: 1300px)": {
                      "right": "-8rem",
                      "left": "auto"
                    },
                    "@media (max-width: 992px)": {
                      "left": "auto",
                      "right": "-90px",
                      "border-radius": "18%"
                    }
                  },
                  "@media (max-width: 992px)": {
                    "right": "-90px",
                    "border-radius": "18%"
                  }
                },
                ".icon-decor": {
                  "position": "absolute",
                  "top": "14rem",
                  "left": "-13rem",
                  "width": "100px",
                  "height": "100px",
                  "font-size": "48px",
                  "display": "inline-flex",
                  "align-items": "center",
                  "justify-content": "center",
                  "background-color": "@iconBg",
                  "color": "@icon",
                  "border-radius": "100%",
                  "border": "5px solid @decor",
                  "z-index": "1",
                  "@media (max-width: 1300px)": {
                    "left": "-9.8rem"
                  },
                  "& when not (@reverseContent)": {
                    "left": "auto",
                    "right": "-13rem",
                    "@media (max-width: 1300px)": {
                      "right": "-9.8rem",
                      "left": "auto"
                    }
                  },
                  "@media (max-width: 992px)": {
                    "display": "none"
                  }
                }
              }
            },
            ".title-wrapper": {
              "& when (@showDecor)": {
                "padding-top": "200px"
              },
              "position": "relative",
              "z-index": "1",
              ".mbr-section-title": {
                "margin-bottom": "40px",
                "@media (max-width: 992px)": {
                  "margin-bottom": "20px"
                }
              }
            },
            ".items-wrapper": {
              "position": "relative",
              "z-index": "1",
              "display": "grid",
              "grid-template-columns": "1fr 1fr 1fr",
              "gap": "15px",
              "@media (max-width: 992px)": {
                "grid-template-columns": "1fr",
                "gap": "10px"
              },
              ".item": {
                "border-radius": "16px !important",
                "background-color": "@card",
                "overflow": "hidden",
                ".item-wrapper": {
                  "height": "100%",
                  "display": "flex",
                  "flex-direction": "column",
                  "justify-content": "space-between",
                  ".item-content": {
                    "padding": "40px 40px 100px",
                    "@media (max-width: 992px)": {
                      "padding": "20px 20px 30px"
                    },
                    ".icon-wrapper": {
                      "margin-bottom": "25px",
                      ".mbr-iconfont": {
                        "width": "46px",
                        "height": "46px",
                        "font-size": "30px",
                        "display": "inline-flex",
                        "align-items": "center",
                        "justify-content": "center",
                        "background-color": "@iconBg",
                        "color": "@icon",
                        "border-radius": "50%"
                      }
                    },
                    ".item-title": {
                      "margin-bottom": "20px"
                    },
                    ".mbr-text": {
                      "margin-bottom": "0"
                    }
                  },
                  ".item-img": {
                    "padding-left": "60px",
                    "img": {
                      "height": "220px",
                      "object-fit": "cover",
                      "border-radius": "0 !important",
                      "border-top-left-radius": "16px !important"
                    }
                  }
                }
              }
            },
            ".mbr-section-title": {
              "color": "#232323",
              "text-align": "left"
            },
            ".item-title": {
              "color": "#192227"
            },
            ".mbr-text": {
              "color": "#ffffff"
            },
            ".item-title, .icon-wrapper": {
              "color": "#ffffff"
            }
          },
