        {
          "_name": "features01",
          "_sourceTheme": "planm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features01 planm5\" group=\"Features\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Decoration\" name=\"showDecor\" checked>\n        <input type=\"color\" title=\"Decoration Color\" value=\"#42d1ab\" name=\"decor\" condition=\"showDecor\" selected>\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Card Icon\" name=\"showIcon\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#ffffff\" name=\"icon\" condition=\"showIcon\" selected>\n        <input type=\"color\" title=\"Icon BG\" value=\"#42d1ab\" name=\"iconBg\" condition=\"showIcon\" selected>\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showText\" checked>\n        <input type=\"color\" title=\"Card BG\" value=\"#2978b4\" name=\"card\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/planm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/planm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-10 card\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\"><b>Our mission is to attract top-tier talent and deliver hands-on training to cultivate world-class Telemarketer and Virtual Assistants.</b></h2>\n                </div>\n                <div class=\"items-wrapper\">\n                    <div class=\"item features-image\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"item-content\">\n                                <div class=\"icon-wrapper\">\n                                    <span mbr-icon class=\"mbr-iconfont mbri-edit\" mbr-if=\"showIcon\"></span>\n                                </div>\n                                <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title, .icon-wrapper\" mbr-if=\"showCardTitle\">\n                                    Strict Screening Process and Training</h5>\n                                <p class=\"mbr-text mbr-fonts-style\" mbr-if=\"showText\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">\n                                    We meticulously select and train our virtual assistants to ensure they meet the highest standards, guaranteeing top-quality support for your business.\n                                </p>\n                            </div>\n                            <div class=\"item-img\">\n                                <img src=\"@PROJECT_PATH@/assets/images/1-896x751.png\" alt=\"Mobirise Website Builder\">\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"item features-image\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"item-content\">\n                                <div class=\"icon-wrapper\">\n                                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-user-2 mobi-mbri\" mbr-if=\"showIcon\"></span>\n                                </div>\n                                <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title, .icon-wrapper\" mbr-if=\"showCardTitle\">\n                                    Dedicated HR Management</h5>\n                                <p class=\"mbr-text mbr-fonts-style\" mbr-if=\"showText\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">\n                                    Our dedicated Human Resources team oversees the recruitment, onboarding, and professional development of our virtual assistants, ensuring a skilled and motivated workforce.\n                                </p>\n                            </div>\n                            <div class=\"item-img\">\n                                <img src=\"@PROJECT_PATH@/assets/images/2-896x751.png\" alt=\"Mobirise Website Builder\">\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"item features-image\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"item-content\">\n                                <div class=\"icon-wrapper\">\n                                    <span mbr-icon class=\"mbr-iconfont mbrib-protect\" mbr-if=\"showIcon\"></span>\n                                </div>\n                                <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title, .icon-wrapper\" mbr-if=\"showCardTitle\">\n                                    A Team Dedicated for YOU</h5>\n                                <p class=\"mbr-text mbr-fonts-style\" mbr-if=\"showText\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">\n                                    Each client is assigned a dedicated team of virtual assistants who are exclusively focused on meeting your unique needs and delivering personalized, efficient support.\n                                </p>\n                            </div>\n                            <div class=\"item-img\">\n                                <img src=\"@PROJECT_PATH@/assets/images/3-896x751.png\" alt=\"Mobirise Website Builder\">\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"decor-wrapper\" mbr-if=\"showDecor\"></div>\n                <span mbr-icon class=\"mbr-iconfont icon-decor mobi-mbri-success mobi-mbri\" mbr-if=\"showDecor\"></span>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "overflow": "hidden",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0",
                "@media (max-width: 992px)": {
                  "padding": "0"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 16px"
              }
            },
            ".row": {
              "position": "relative",
              "margin": "0 20px",
              "@media (max-width: 992px)": {
                "margin": "0 10px"
              },
              ".card": {
                "position": "relative",
                "padding": "0",
                ".decor-wrapper": {
                  "position": "absolute",
                  "top": "0",
                  "left": "-11rem",
                  "width": "3000px",
                  "height": "2000px",
                  "border": "40px solid @decor",
                  "border-radius": "15rem",
                  "pointer-events": "none",
                  "@media (max-width: 1300px)": {
                    "left": "-8rem"
                  },
                  "& when not (@reverseContent)": {
                    "left": "auto",
                    "right": "-11rem",
                    "@media (max-width: 1300px)": {
                      "right": "-8rem",
                      "left": "auto"
                    },
                    "@media (max-width: 992px)": {
                      "left": "auto",
                      "right": "-90px",
                      "border-radius": "18%"
                    }
                  },
                  "@media (max-width: 992px)": {
                    "right": "-90px",
                    "border-radius": "18%"
                  }
                },
                ".icon-decor": {
                  "position": "absolute",
                  "top": "14rem",
                  "left": "-13rem",
                  "width": "100px",
                  "height": "100px",
                  "font-size": "48px",
                  "display": "inline-flex",
                  "align-items": "center",
                  "justify-content": "center",
                  "background-color": "@iconBg",
                  "color": "@icon",
                  "border-radius": "100%",
                  "border": "5px solid @decor",
                  "z-index": "1",
                  "@media (max-width: 1300px)": {
                    "left": "-9.8rem"
                  },
                  "& when not (@reverseContent)": {
                    "left": "auto",
                    "right": "-13rem",
                    "@media (max-width: 1300px)": {
                      "right": "-9.8rem",
                      "left": "auto"
                    }
                  },
                  "@media (max-width: 992px)": {
                    "display": "none"
                  }
                }
              }
            },
            ".title-wrapper": {
              "& when (@showDecor)": {
                "padding-top": "200px"
              },
              "position": "relative",
              "z-index": "1",
              ".mbr-section-title": {
                "margin-bottom": "40px",
                "@media (max-width: 992px)": {
                  "margin-bottom": "20px"
                }
              }
            },
            ".items-wrapper": {
              "position": "relative",
              "z-index": "1",
              "display": "grid",
              "grid-template-columns": "1fr 1fr 1fr",
              "gap": "15px",
              "@media (max-width: 992px)": {
                "grid-template-columns": "1fr",
                "gap": "10px"
              },
              ".item": {
                "border-radius": "16px !important",
                "background-color": "@card",
                "overflow": "hidden",
                ".item-wrapper": {
                  "height": "100%",
                  "display": "flex",
                  "flex-direction": "column",
                  "justify-content": "space-between",
                  ".item-content": {
                    "padding": "40px 40px 100px",
                    "@media (max-width: 992px)": {
                      "padding": "20px 20px 30px"
                    },
                    ".icon-wrapper": {
                      "margin-bottom": "25px",
                      ".mbr-iconfont": {
                        "width": "46px",
                        "height": "46px",
                        "font-size": "30px",
                        "display": "inline-flex",
                        "align-items": "center",
                        "justify-content": "center",
                        "background-color": "@iconBg",
                        "color": "@icon",
                        "border-radius": "50%"
                      }
                    },
                    ".item-title": {
                      "margin-bottom": "20px"
                    },
                    ".mbr-text": {
                      "margin-bottom": "0"
                    }
                  },
                  ".item-img": {
                    "padding-left": "60px",
                    "img": {
                      "height": "220px",
                      "object-fit": "cover",
                      "border-radius": "0 !important",
                      "border-top-left-radius": "16px !important"
                    }
                  }
                }
              }
            },
            ".mbr-section-title": {
              "color": "#232323",
              "text-align": "left"
            },
            ".item-title": {
              "color": "#192227"
            },
            ".mbr-text": {
              "color": "#ffffff"
            },
            ".item-title, .icon-wrapper": {
              "color": "#ffffff"
            }
          },
